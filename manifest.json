{
  "manifest_version": 3,
  "name": "HProxie",
  "description": "Bring your own proxy to your browser.",
  "version": "1.3",
  "icons": {
    "64": "images/icon64.png"
  },
  "permissions": [
    "proxy",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}